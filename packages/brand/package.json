{
  "name": "@henrikkvamme/brand",
  "version": "1.0.2",
  "description": "Reusable brand elements for Henrik Kvamme projects - logo, footer, and theme components",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
