    "format": "biome format --write .",
    "lint": "biome lint .",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "components",
    "brand",
    "logo",
    "footer",
    "theme",
    "henrik-kvamme"
  ],
